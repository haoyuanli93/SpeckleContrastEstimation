   "outputs": [],
   "source": [
    "# Load the SAXS data\n",
    "file_location = (\"C:/Users/haoyuan/Desktop/PosDoc/WaterContrastEstimation/JupyterNotebook_v4/SAXS_data/H20_25_0C_002_0bar_S021_tot.dat\")\n",
    "data = np.loadtxt(file_location)"
