   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Calculate the Differential Cross Section"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "my_density = 0.2 # g / ml\n",
    "\n",
    "my_molecule =  util.get_molecule_from_pdb(\"../pdb_files/trioxane.pdb\") #MoleculeZoo.molecule_zoo['H2O']\n",
    "atten_length = IntensityEstimation.get_attenuation_length_cm(molecule_structure=my_molecule,\n",
    "                                                             photon_energy_keV=15,\n",
    "                                                             density=my_density)\n",
    "atten_length *= 1e-2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The differential cross section at 30 C is 1.39 [1/m]\n"
     ]
    }
   ],
   "source": [
    "photon_energy = 15.  # keV\n",
    "I_in = 2.5  *1e12  # incoming xray flux [photons/sec] at 14keV\n",
    "l    = 2.5         # sample-to-detector distance [m]\n",
    "p    = 172. *1e-6  # detector pixel size [m]\n",
    "d    = 4.   *1e-3  # sample thickness [m]\n",
    "\n",
    "# solid angle covered by a single detector pixel\n",
    "omega = (p / l)**2 \n",
    "\n",
    "# Get effective sampe thickness\n",
    "d_eff = atten_length * (1 - np.exp( - d / atten_length))\n",
    "\n",
    "# Get the scatterred intensity at the specified Q value\n",
    "q_idx = np.argmin(np.abs(data[:,0] - expParams['Q'])) \n",
    "I_out = data[q_idx, 1]\n",
    "\n",
    "# Get the cross section\n",
    "diff_cross = 1.39 #I_out / (omega * d_eff * I_in) #  30 C\n",
    "\n",
    "print('The differential cross section at 30 C is {:.2f} [1/m]'.format(diff_cross))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Get the estimated scattering intensity at LCLS"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "For each incident pulse, there are 1.58e+08 photons in each branch\n"
     ]
    }
   ],
   "source": [
    "incident_pulse_energy = 30.  # uJ\n",
    "incident_pulse_energy_in_eV = incident_pulse_energy *  6241506479963.2\n",
    "\n",
    "efficiency_per_branch = 0.008 # Transmission efficiency per branch of the split delay\n",
    "\n",
    "photon_per_pulse = incident_pulse_energy_in_eV * efficiency_per_branch / (expParams['photon_energy_keV'] * 1000)   # photon number per branch per \n",
    "print(\"For each incident pulse, there are {:.2e} photons in each branch\".format(photon_per_pulse))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "done\n"
     ]
    }
   ],
   "source": [
    "# Get the new attenuation\n",
    "atten_length = IntensityEstimation.get_attenuation_length_cm(molecule_structure=my_molecule,\n",
    "                                                             photon_energy_keV=expParams[\"photon_energy_keV\"],\n",
    "                                                             density=my_density)\n",
    "atten_length *= 1e-2\n",
    "\n",
    "inty_map = np.zeros((num, num)) #scattering intensity map\n",
    "\n",
    "for idx1 in range(num):    \n",
    "    for idx2 in range(num):\n",
    "        \n",
    "        #estimated scattering intensity on a single detector pixel [photons/pixels/shot]\n",
    "        inty_map[idx1, idx2] = IntensityEstimation.get_scatter_intensity_with_differetial_crosssection(\n",
    "            diff_cross_list = diff_cross,\n",
    "            atten_length = atten_length,\n",
    "            sample_thickness = thickness_list[idx1] * 1e-6,\n",
    "            pixel_size = expParams['pixel_size'] * 1e-6,\n",
    "            detector_distance = distance_list[idx2],\n",
    "            incident_photon_count = photon_per_pulse)\n",
    "\n",
    "print('done')"
   ]
  },
