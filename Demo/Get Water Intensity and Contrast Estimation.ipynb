    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import matplotlib\n",
    "from matplotlib.ticker import ScalarFormatter, NullFormatter\n",
    "\n",
    "plt.rcParams.update({'font.size': 14})\n",
    "matplotlib.rcParams['xtick.minor.size'] = 0\n",
    "matplotlib.rcParams['xtick.minor.width'] = 0\n",
    "\n",
    "import sys\n",
    "sys.path.append(\"..\")\n",
    "\n",
    "from ContrastEstimation import util, IntensityEstimation, ContrastEstimation, MoleculeZoo"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Define the Incident Beam"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "expParams = {}\n",
    "\n",
    "# Photon energy and bandwidth\n",
    "expParams['photon_energy_keV'] = 9.5  # Beam central photon energy in keV\n",
    "expParams['bandwidth_keV'] = 0.3 * 1e-3 # Beam bandwidth in keV\n",
    "\n",
    "# Beam size (focusing spot size)\n",
    "expParams['beam_size_out_plane'] = 2.  # (um) Beam size on the sample perpendicular to the diffraction plane\n",
    "expParams['beam_size_in_plane'] = 2.  # (um) Beam size on the sample within the diffraction plane\n",
    "\n",
    "# Beam transverse coherence length (No need to change for LCLS beams)\n",
    "expParams['trans_coh_length_out_plane'] = 30.  # (um) Beam coherence length perpendicular to the diffraction plane\n",
    "expParams['trans_coh_length_in_plane'] = 30.  # (um) Beam coherence length within to the diffraction plane\n",
    "\n",
    "# Detector pixel parameter\n",
    "expParams['pixel_size'] = 50.0  # (um) detector pixel size\n",
    "\n",
    "# Q value of interests\n",
    "expParams['Q'] = 2.0 # 2 pi / length scale  # The unit is A^-1\n",
    "\n",
    "# Scattering Geometry: Gamma = 0 means forward scattering, 1 for back scattering.\n",
    "# Back scattering is only possible with crystal\n",
    "expParams['gamma'] = 0.0\n",
    "\n",
    "##########################################################\n",
    "#                Parameters to scan.\n",
    "##########################################################\n",
    "expParams['sample_thickness'] = 50.  # (um) Sample thickness\n",
    "expParams['detector_distance'] = 2.0  # (m) Detector distance\n",
    "\n",
    "# Automatically calculate other quantities to facilicate the calculation\n",
    "expParams = util.auto_update_expParams(expParams)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Get the speckle contrast map"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "scrolled": false
   },
   "outputs": [],
   "source": [
    "num = 100\n",
    "holder = np.zeros((num, num, 5))\n",
    "\n",
    "# thickness\n",
    "thickness_list = np.logspace(1, 4, num)\n",
    "\n",
    "# detector distances\n",
    "distance_list = np.logspace(0, 1, num)\n",
    "\n",
    "# Loop through all conditions\n",
    "for idx1 in range(num):\n",
    "    for idx2 in range(num):\n",
    "        expParams['sample_thickness'] = thickness_list[idx1]\n",
    "        expParams['detector_distance'] = distance_list[idx2]\n",
    "        holder[idx1, idx2, :] = ContrastEstimation.get_contrast(expParams)\n",
    "        \n",
    "beta_map = holder[:,:,-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 2 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, axis = plt.subplots(ncols=1, nrows=1)\n",
    "\n",
    "img1 = axis.imshow(beta_map,\n",
    "                   aspect='auto',\n",
    "                   cmap='jet')\n",
    "axis.set_xlabel(\"Detector distance (m)\")\n",
    "axis.set_ylabel(\"Sample thickness (um)\")\n",
    "\n",
    "axis.set_xticks([100 * np.log10(2 * x + 1) for x in range(4)])\n",
    "axis.set_xticklabels([\"{}\".format(2 * x + 1) for x in range(4)])\n",
    "axis.set_yticks([33 * x for x in range(4)])\n",
    "axis.set_yticklabels([\"$10^{}$\".format(x + 1) for x in range(4)])\n",
    "\n",
    "fig.colorbar(img1, ax=axis)\n",
    "\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Get detector pixel number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "pixel_num_holder = np.zeros(100)  # calculate the number of pixels within the range\n",
    "for i,l in enumerate(distance_list):    \n",
    "    q_map = ContrastEstimation.get_detector_q_map(theta0 = np.arcsin(expParams['Q'] /2. / expParams['incident_wavevec']),\n",
    "                               n_pix = 1500,\n",
    "                               pix_size = expParams['pixel_size'] / 1e6,\n",
    "                               det_dist = l,\n",
    "                               incident_wavevec_A = expParams['incident_wavevec'])\n",
    "    pixel_num_holder[i] = ContrastEstimation.get_pixel_num_in_Q(q_map,\n",
    "                                                  Q_min = expParams['Q'] * 0.99,\n",
    "                                                  Q_max = expParams['Q'] * 1.01)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.plot(distance_list, pixel_num_holder)\n",
    "plt.xlabel(\"Detector distance (m)\", fontsize=15)\n",
    "plt.ylabel(\"Pixel number\", fontsize=15)\n",
    "plt.title(\"Pixel number vs Detector distance\", fontsize=15)\n",
    "plt.show()"
   ]
  },
