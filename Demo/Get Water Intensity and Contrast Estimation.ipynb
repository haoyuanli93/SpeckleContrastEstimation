    "ax[2].set_title('signal-to-noise')\n",
    "\n",
    "ax[2].set_xticks([100 * np.log10(x + 1) for x in range(4)])\n",
    "ax[2].set_xticklabels([\"{}\".format(x + 1) for x in range(4)])\n",
    "ax[2].set_yticks([33 * x for x in range(4)])\n",
    "ax[2].set_yticklabels([\"$10^{}$\".format(x + 1) for x in range(4)])\n",
    "\n",
    "fig.colorbar(im,ax=ax[2])\n",
    "\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
